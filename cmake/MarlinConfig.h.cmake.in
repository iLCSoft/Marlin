#define MARLIN_VERSION_GE( MAJV , MINV , PLEV )  ( (MARLIN_MAJOR_VERSION > MAJV) || ( (MARLIN_MAJOR_VERSION == MAJV) && (MARLIN_MINOR_VERSION > MINV) ) || ( (MARLIN_MAJOR_VERSION == MAJV) && (MARLIN_MINOR_VERSION == MINV) && (MARLIN_PATCH_LEVEL >= PLEV) ) )

